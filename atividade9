nota1 = input ("escreva a primeira nota ")
nota2 = input ("escreva a segunda nota ")
nota3 = input ("escreva a terceira nota ")
media = (float(nota1) + float(nota2) + float(nota3)) / 3 
if media <= 6.9 :
    print ("aluno reprovado com media " + "'D'")
elif media >=9:
    print ("aluno passou perfeitamente " + "'A'")
elif media >=8:
    print ("aluno passou " + "'B'" )
else :
    print ("aluno passou " + "'C'" )